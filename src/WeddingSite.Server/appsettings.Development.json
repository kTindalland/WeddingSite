{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "",
          "apiKey": "none"
        }
      },
      {
        "Name": "Console"
      }
      
    ],
    "Enrich": [ "WithEnvironmentName" ],
    "Application": "WeddingSite"
  },
  "AllowedHosts": "*"
}