{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.MongoDB"
    ],
    "MinimumLevel": "Error",
    "WriteTo": [
      {
        "Name": "MongoDBBson",
        "Args": {
          "databaseUrl": "",
          "collectionName": "logs",
          "cappedMaxSizeMb": "1024",
          "cappedMaxDocuments": "50000",
          "rollingInterval": "Month"
        }
      }
    ],
    "Enrich" :  ["EnvironmentName"],
    "Application": "WeddingSite"
  },
  "AllowedHosts": "*"
}